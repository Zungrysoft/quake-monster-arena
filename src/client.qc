/*
============
SelectSpawnPoint

Returns the entity to spawn at
============
*/
entity() SelectSpawnPoint =
{
	local	entity spot;
	
// testinfo_player_start is only found in regioned levels
	spot = find (world, classname, "testplayerstart");
	if (spot)
		return spot;
	
	//On the start map, if returning from a level, skip the skill select room
	if (world.model == "maps/start.bsp" && parm16 & 1) {
		lastspawn = find(lastspawn, classname, "info_player_start2");
		if (lastspawn == world)
			lastspawn = find (lastspawn, classname, "info_player_start2");
		return lastspawn;
	}
	//In deathmatch, start each player at the skill select room
	else if (deathmatch) {
		if (!self.arena_team) {
			lastspawn = find(lastspawn, classname, "info_player_start");
			if (lastspawn == world)
				lastspawn = find (lastspawn, classname, "info_player_start");
			return lastspawn;
		}
		else if (self.arena_team == 1) {
			lastspawn1 = find(lastspawn1, classname, "info_player_team1");
			if (lastspawn1 == world)
				lastspawn1 = find (lastspawn1, classname, "info_player_team1");
			return lastspawn1;
		}
		//Team 2
		else {
			lastspawn2 = find(lastspawn2, classname, "info_player_team2");
			if (lastspawn2 == world)
				lastspawn2 = find (lastspawn2, classname, "info_player_team2");
			return lastspawn2;
		}
	} else {
		//In singleplayer and coop games, you always play on team 1
		lastspawn = find(lastspawn, classname, "info_player_team1");
		if (lastspawn == world)
			lastspawn = find (lastspawn, classname, "info_player_team1");
		return lastspawn;
	}
	
	spot = find (world, classname, "info_player_start");
	if (!spot)
		error ("PutClientInServer: no info_player_start on level");
	
	return spot;
};

