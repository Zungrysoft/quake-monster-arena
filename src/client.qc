				bprint ("\n");
				return;
			}
			if (attacker.classname == "trigger_changelevel")
			{
				bprint (LOC_DEATH_LEAVE);
				bprint ("\n");
				return;
			}

			// in-water deaths
			rnum = targ.watertype;
			if (rnum == -3)
			{
				if (random() < 0.5)
				{
					bprint (LOC_DEATH_DROWN1);
				}
				else if (random() < 0.9)
				{
					bprint (LOC_DEATH_DROWN2);
				}
				else 
